pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                echo 'maven build'
            }
        }
        stage('Test'){
            steps{
                echo 'test successful'
            }
        }
        stage('Docker build'){
            steps{
                echo 'Docker imgae build successful'
            }
        }
        stage('deploy'){
            steps{
                echo 'deployment successful'
            }
        }
    }
}
