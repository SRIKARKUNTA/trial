pipeline{
    agent any
    stages{
        stage('build'){
            steps{
            echo 'maven build'
        }
        }
         stage('Test'){
            steps{
            echo 'maven build'
        }
        stage('Docker build'){
            steps{
            echo 'Docker build successful'
        }
        stage('Deploy'){
            steps{
            echo 'Deploy successful'
        }
    }

}
         }
    }
}
    
